{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "price": {
      "type": "float",
      "required": true,
      "default": 0
    },
    "size": {
      "type": "integer"
    },
    "city": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "posts"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "posts"
    }
  }
}
